{
  "name": "techbikers.com",
  "version": "2.0.0",
  "description": "The techbikers.com webite for ride administration, rider registration",
  "author": "Michael Willmott <mwillmott@gmail> (http://mwillmott.co)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Techbikers/techbikers/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Techbikers/techbikers.git"
  },
  "scripts": {
    "start": "better-npm-run watch",
    "build": "better-npm-run build",
    "lint": "eslint client"
  },
  "betterScripts": {
    "watch": {
      "command": "webpack-dev-server --inline --hot --history-api-fallback --config webpack.dev.config.js --port 3000"
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack.prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
