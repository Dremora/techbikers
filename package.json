    "moment": "^2.10.3",
    "newforms": "^0.12.1",
    "react": "^0.13.2",
    "react-document-title": "latest",
    "react-intl": "^1.2.0",
    "react-router": "^0.13.3",
    "superagent": "^1.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.2.3",
    "babel-core": "^6.4.0",
    "babel-eslint": "^5.0.0-beta10",
    "babel-loader": "^6.2.1",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
